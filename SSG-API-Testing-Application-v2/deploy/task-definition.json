{
    "family": "app-v1",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "app",
            "image": "",
            "portMappings": [
                {
                    "containerPort": 8502,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "disableNetworking": false,
            "privileged": true,
            "readonlyRootFilesystem": false,
            "interactive": true,
            "pseudoTerminal": true
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:8502/ || exit 1"],
        "interval": 60,
        "timeout": 15,
        "retries": 3
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
